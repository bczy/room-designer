# AR Furniture Previewer Environment Variables
# Copy this file to .env and fill in your values
# NEVER commit .env to version control (FR-045)

# 8th Wall Configuration (Required for AR features)
EIGHTH_WALL_API_KEY=your_8th_wall_api_key_here

# Lightship Configuration (Optional - for VPS/scanning)
LIGHTSHIP_API_KEY=your_lightship_api_key_here

# App Environment (development | staging | production)
APP_ENV=development

# Debug flags (development only)
DEBUG_AR=true
